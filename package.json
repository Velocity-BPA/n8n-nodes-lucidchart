{
  "name": "n8n-nodes-lucidchart",
  "version": "1.0.0",
  "description": "A comprehensive n8n community node for Lucidchart API providing 10 resources and 50+ operations for document management, collaboration, sharing, and diagramming automation.",
  "license": "BUSL-1.1",
  "author": {
    "name": "Velocity BPA",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Velocity-BPA/n8n-nodes-lucidchart.git"
  },
  "homepage": "https://velobpa.com",
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "lucidchart",
    "lucid",
    "diagram",
    "flowchart",
    "collaboration",
    "automation",
    "document-management",
    "diagramming"
  ],
  "main": "dist/nodes/Lucidchart/Lucidchart.node.js",
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/LucidchartApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Lucidchart/Lucidchart.node.js"
    ]
  },
  "scripts": {
    "build": "tsc && npm run copy:icons",
    "copy:icons": "cp nodes/Lucidchart/*.svg dist/nodes/Lucidchart/ 2>/dev/null || true",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.ts\"",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "jest": "^29.7.0",
    "n8n-workflow": "^1.30.0",
    "prettier": "^3.2.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "n8n-workflow": "^1.30.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist"
  ]
}
